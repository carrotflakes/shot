(defpackage shot.parser
  (:use :cl
        :snaky)
  (:export :parse))
(in-package :shot.parser)

(defrule statements
    (many statement))

(defrule statement
    (@ (or (and (ret :definition-statement)
                definition)
           (and (ret :expression-statement)
                (expression (ret :expression))))))

(defrule definition
    (or function-definition constant-definition))

(defrule function-definition
    (@ (and (ret :function-definition)
            identifier
            ws
            block)))

(defrule constant-definition
    (@ (and (ret :constant-definition)
            (expression (ret :pattern))
            ws "=" ws
            (expression (ret :expression)))))

(defrule patterns
    (or (and "(" (expressions (ret :pattern)) ")")
        (@ (expression (ret :pattern)))))

(defrule (expression mode)
    (operator-system mode))

(defrule (expressions mode)
    (many (operator-system mode)))

(defrule (operator-system mode)
    (operator-1 mode))

(defrule (operator-1 mode)
    (or (mod (@ (and (operator-1 mode)
                     ws
                     (cap (or "|"))
                     ws
                     (operator-2 mode)))
             (lambda (list)
               (list :apply-expression (second list) (first list) (third list))))
        (operator-2 mode)))

(defrule (operator-2 mode)
    (or (mod (@ (and (operator-2 mode)
                     ws
                     (cap (or "?"))
                     ws
                     (operator-3 mode)))
             (lambda (list)
               (list :apply-expression (second list) (first list) (third list))))
        (operator-3 mode)))

(defrule (operator-3 mode)
    (or (mod (@ (and (operator-3 mode)
                     ws
                     (cap (or "&&" "||"))
                     ws
                     (operator-4 mode)))
             (lambda (list)
               (list :apply-expression (second list) (first list) (third list))))
        (operator-4 mode)))

(defrule (operator-4 mode)
    (or (mod (@ (and (operator-5 mode)
                     ws
                     (cap (or "==" "!=" ">=" "<=" ">" "<"))
                     ws
                     (operator-5 mode)))
             (lambda (list)
               (list :apply-expression (second list) (first list) (third list))))
        (operator-5 mode)))

(defrule (operator-5 mode)
    (or (mod (@ (and (operator-5 mode)
                     ws
                     (cap (or "+" "-"))
                     ws
                     (operator-6 mode)))
             (lambda (list)
               (list :apply-expression (second list) (first list) (third list))))
        (operator-6 mode)))

(defrule (operator-6 mode)
    (or (mod (@ (and (operator-6 mode)
                     ws
                     (cap (or "*" "/" "%"))
                     ws
                     (operator-7 mode)))
             (lambda (list)
               (list :apply-expression (second list) (first list) (third list))))
        (operator-7 mode)))

(defrule (operator-7 mode)
    (or (@ (and (ret :at-pattern)
                (operator-8 mode)
                ws "@" ws
                (operator-8 mode)))
        (operator-8 mode)))

(defrule (operator-8 mode)
    (or (@ (and (ret :apply-expression)
                (cap "-") ws
                (applies mode)))
        (applies mode)))

(defrule (applies mode)
    (or (@ (and (wst (grd mode (lambda (x) (eq x :expression))))
                (ret :apply-expression)
                (or (and (ret "()")
                         (applies mode)
                         "(" ws
                         (expressions mode)
                         ws ")")
                    (and (ret "[]")
                         (applies mode)
                         "[" ws
                         (expression mode)
                         ws "]")
                    (and (ret "[]")
                         (applies mode)
                         ws "." ws
                         (@ (and (ret :value-expression)
                                 (@ (and (ret :string-value)
                                         identifier))))))))
        (@ (and (wst (grd mode (lambda (x) (eq x :pattern))))
                (ret :apply-expression)
                (ret "()")
                (applies mode)
                "(" ws
                (expressions mode)
                ws ")"))
        (factor mode)))

(defrule (factor mode)
    (or (and (wst (grd mode (lambda (x) (eq x :expression))))
             (or (and "(" ws (operator-system mode) ws ")")
                 (@ (or (and (ret :value-expression)
                             value-expression)
                        (and (ret :variable-expression)
                             identifier)
                        (and (ret :list-expression)
                             "[" ws (expressions mode) ws "]")
                        (and (ret :object-expression)
                             "{" ws (many (member mode)) ws "}")
                        (and (ret :closure-expression)
                             "[" ws block ws "]")))
                 (template-literal mode)))
        (and (wst (grd mode (lambda (x) (eq x :pattern))))
             (or (and "(" ws (operator-system mode) ws ")")
                 (@ (or (and (ret :value-expression)
                             value-expression)
                        (and (ret :variable-expression)
                             identifier)
                        (and (ret :list-expression)
                             "[" ws (expressions mode) ws "]")
                        (and (ret :object-expression)
                             "{" ws (many (member mode)) ws "}")))))))

(defrule (member mode)
    (or (@ (and (ret :property-member)
                (or identifier string-literal)
                ws ":" ws
                (expression mode)))
        (mod (@ (and (ret :property-member)
                     identifier))
             (lambda (x)
               (append x (list (list :variable-expression (second x))))))
        (@ (and (ret :spread-member)
                "..." ws (expression mode)))
        (@ (and (wst (grd mode (lambda (x) (eq x :expression))))
                (ret :computed-property-member)
                "[" ws (expression mode) ws "]"
                ws ":" ws
                (expression mode)))))

(defrule block
    (many-1 (@ (and patterns ws "=" ws (expression (ret :expression))))))

(defrule guard-clauses
    (@ (and (ret :guard-clauses)
            (many-1 guard-clause))))

(defrule guard-clause
    (@ (and "|" ws (expression (ret :expression))
            ws "=" ws (expression (ret :expression)))))

(defrule value-expression
    (@ (or (and (ret :number-value)
                numeric-literal)
           (and (ret :string-value)
                (or string-literal
                    (and ":" identifier)))
           (and (ret :bool-value)
                (or (and "true" (ret :true))
                    (and "false" (ret :false))))
           (and (ret :null-value)
                "null"))))

